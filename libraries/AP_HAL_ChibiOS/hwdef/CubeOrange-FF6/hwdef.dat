# hw definition file for processing by chibios_hwdef.py
# CubeOrange-FF6 board variant for FireFLY6 tiltrotor

include ../CubeOrange/hwdef.dat

undef USB_STRING_PRODUCT
undef USB_STRING_MANUFACTURER
undef APJ_BOARD_ID
undef USB_PRODUCT

# USB setup
USB_PRODUCT 0x1058
USB_STRING_MANUFACTURER "CubePilot"
USB_STRING_PRODUCT "CubeOrange-FF6"

APJ_BOARD_ID 1034

# Use CubeOrange bootloader
BOOTLOADER_DEV_LIST CubeOrange
